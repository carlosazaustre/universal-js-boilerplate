{
  "engines": {
    "node": ">=4.1 <5",
    "npm": ">=3.1 <4"
  },
  "name": "universal-js-boilerplate",
  "version": "0.0.1",
  "description": "A boilerplate to start universal (isomorphic) web applications",
  "main": "./src/app/server.js",
  "scripts": {
    "start": "nodemon --exec babel-node -- ./src/app/server.js",
    "build": "TARGET=build webpack",
    "dev": "TARGET=dev webpack-dev-server --progress --colors --hot --inline --history-api-fallback --content-base www",
    "deploy": "TARGET=build webpack -p -d"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/carlosazaustre/universal-js-boilerplate"
  },
  "keywords": [
    "boilerplate",
    "web development",
    "frontend",
    "javascript",
    "es6",
    "ecmascript 6",
    "react",
    "isomorphic"
  ],
  "author": "Carlos Azaustre <cazaustre@gmail.com> (https://carlosazaustre.es/blog)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/carlosazaustre/universal-js-boilerplate/issues"
  },
  "homepage": "https://github.com/carlosazaustre/universal-js-boilerplate",
